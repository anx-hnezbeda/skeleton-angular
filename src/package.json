{
  "name": "angular-demo",
  "version": "1.0.0",
  "description": "Angular Demo",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test karma start ./karma/karma.conf.js",
    "build": "webpack --config webpack/webpack.build.js",
    "build:development": "NODE_ENV=development npm run build --",
    "build:production": "NODE_ENV=production npm run build --",
    "runserver": "NODE_ENV=development webpack-dev-server --config webpack/webpack.build.js --progress --profile --watch",
    "runserver:hmr": "npm run runserver -- --inline --hot",
    "makemessages:extract": "ngx-translate-extract --input ./app --output ./app/locales/locales.pot --marker translate --format pot --clean --sort",
    "makemessages:merge": "cd ./app/locales/; for po in *.po; do msgmerge \"$po\" locales.pot > \"merged_${po}\" && mv \"merged_${po}\" \"${po}\"; done",
    "makemessages": "npm run makemessages:extract && npm run makemessages:merge",
    "generatedocs": "cd app/; compodoc --tsconfig ../tsconfig.json --output ../../docs --theme readthedocs --name 'Angular Demo'"
  },
  "keywords": [
    "angular",
    "demo"
  ],
  "author": "ANEXIA Internetdienstleisungs GmbH",
  "license": "PROPRIETARY SOFTWARE",
