{
  "name": "{{ cookiecutter.project_slug }}",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "app-start": "ng serve --host 0.0.0.0 --port 8080",
    "app-start-poll": "ng serve --host 0.0.0.0 --port 8080 --poll 1000",
    "app-start-hmr": "ng serve --host 0.0.0.0 --port 8080 -c=hmr",
    "app-start-hmr-poll": "ng serve --host 0.0.0.0 --port 8080 -c=hmr --poll 1000",
    "app-build-dev": "node helpers/version.js && ng build --base-href /",
    "app-build-stage": "node helpers/version.js && ng build -c=staging --base-href /",
    "app-build-prod": "node helpers/version.js && ng build -c=production --base-href /",
    "test": "node helpers/version.js && ng test --watch=false",
    "test-watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "makemessages": "ngx-translate-extract -i ./projects/{{ cookiecutter.project_slug }}/app/ -o ./projects/{{ cookiecutter.project_slug }}/assets/locales/*.json",
    "generatedocs": "./node_modules/.bin/compodoc -p ./projects/{{ cookiecutter.project_slug }}/tsconfig.app.json -d ./dist/docs",
    "storybook-start": "start-storybook --ci -p 9001 -h 0.0.0.0 -c ./projects/{{ cookiecutter.project_slug }}/storybook/config",
    "storybook-build": "node helpers/version.js && build-storybook -c ./projects/{{ cookiecutter.project_slug }}/storybook/config -o ./dist/storybook",
    "postinstall": "node helpers/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "@ng-select/ng-select": "^3.7.3",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/storage-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@swimlane/ngx-datatable": "^16.0.3",
    "ngx-anx-forms": "^0.2.2",
    "@types/lodash": "^4.14.149",
    "@uirouter/angular": "^5.0.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.4",
    "date-fns": "^2.10.0",
    "lodash": "^4.17.15",
    "ngx-resource-factory": "1.0.0",
    "ngx-toastr": "^11.3.3",
    "ngx-ui-router-url-type-factory": "0.0.3",
    "rxjs": "6.5.4",
    "rxjs-compat": "6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^9.0.6",
    "@angularclass/hmr": "^2.1.3",
    "@babel/core": "^7.8.7",
    "@compodoc/compodoc": "^1.1.9",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@storybook/addon-actions": "~5.3.14",
    "@storybook/addon-knobs": "~5.3.14",
    "@storybook/addon-notes": "~5.3.14",
    "@storybook/addon-options": "~5.3.14",
    "@storybook/angular": "~5.3.14",
    "@types/jasmine": "~3.5.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~13.9.0",
    "babel-loader": "^8.0.5",
    "core-js": "^3.6.4",
    "codelyzer": "~5.2.1",
    "css-loader": "^3.4.2",
    "git-describe": "^4.0.4",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "moment": "^2.24.0",
    "protractor": "~5.4.3",
    "puppeteer": "^2.1.1",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.5.3"
  }
}
