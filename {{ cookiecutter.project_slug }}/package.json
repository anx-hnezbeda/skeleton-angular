{
  "name": "{{ cookiecutter.project_slug }}",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "app-start": "ng serve --host 0.0.0.0 --port 8080",
    "app-start-poll": "ng serve --host 0.0.0.0 --port 8080 --poll 1000",
    "app-start-hmr": "ng serve --host 0.0.0.0 --port 8080 -c=hmr",
    "app-start-hmr-poll": "ng serve --host 0.0.0.0 --port 8080 -c=hmr --poll 1000",
    "app-build-dev": "node helpers/version.js && ng build --base-href /",
    "app-build-stage": "node helpers/version.js && ng build -c=staging --base-href /",
    "app-build-prod": "node helpers/version.js && ng build -c=production --base-href /",
    "test": "node helpers/version.js && ng test --watch=false",
    "test-watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "makemessages": "ngx-translate-extract -i ./projects/{{ cookiecutter.project_slug }}/app/ -o ./projects/{{ cookiecutter.project_slug }}/assets/locales/*.json",
    "generatedocs": "./node_modules/.bin/compodoc -p ./projects/{{ cookiecutter.project_slug }}/tsconfig.app.json -d ./dist/docs",
    "postinstall": "node helpers/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@ng-bootstrap/ng-bootstrap": "^5.3.1",
    "@ng-select/ng-select": "^3.7.3",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^5.0.0",
    "@ngxs/storage-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.1",
    "@swimlane/ngx-datatable": "^16.1.1",
    "ngx-anx-forms": "^0.2.2",
    "@types/lodash": "^4.14.168",
    "@uirouter/angular": "^5.0.0",
    "bootstrap": "^4.6.0",
    "core-js": "^3.9.1",
    "date-fns": "^2.19.0",
    "lodash": "^4.17.21",
    "ngx-resource-factory": "1.0.0",
    "ngx-toastr": "^11.3.3",
    "ngx-ui-router-url-type-factory": "0.0.3",
    "rxjs": "6.6.3",
    "rxjs-compat": "6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular/cli": "~8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^11.2.7",
    "@angularclass/hmr": "^3.0.0",
    "@babel/core": "^7.13.10",
    "@compodoc/compodoc": "^1.1.9",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@types/jasmine": "~3.6.8",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~14.14.36",
    "babel-loader": "^8.2.2",
    "core-js": "^3.9.1",
    "codelyzer": "~6.0.1",
    "css-loader": "^4.3.0",
    "git-describe": "^4.0.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.1.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "moment": "^2.29.1",
    "protractor": "~7.0.0",
    "puppeteer": "^7.1.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.5.3"
  }
}
