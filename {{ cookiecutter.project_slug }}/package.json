{
  "name": "{{ cookiecutter.project_slug }}",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "app-start": "ng serve --host 0.0.0.0 --port 8080",
    "app-start-hmr": "ng serve --host 0.0.0.0 --port 8080 -c=hmr",
    "app-build-dev": "node helpers/version.js && ng build --base-href /",
    "app-build-stage": "node helpers/version.js && ng build -c=staging --base-href /",
    "app-build-prod": "node helpers/version.js && ng build -c=production --base-href /",
    "test": "node helpers/version.js && ng test --watch=false",
    "test-watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "makemessages": "ngx-translate-extract -i ./src/app/ -o ./src/assets/locales/*.json",
    "generatedocs": "./node_modules/.bin/compodoc -p ./src/tsconfig.app.json -d ./dist/docs",
    "storybook-start": "start-storybook -p 9001 -h 0.0.0.0 -c ./src/storybook/config",
    "storybook-build": "node helpers/version.js && build-storybook -c ./src/storybook/config -o ./dist/storybook",
    "postinstall": "node helpers/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.9",
    "@angular/common": "^6.1.9",
    "@angular/compiler": "^6.1.9",
    "@angular/core": "^6.1.9",
    "@angular/forms": "^6.1.9",
    "@angular/http": "^6.1.9",
    "@angular/platform-browser": "^6.1.9",
    "@angular/platform-browser-dynamic": "^6.1.9",
    "@angular/router": "^6.1.9",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@fortawesome/angular-fontawesome": "^0.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@ng-bootstrap/ng-bootstrap": "^3.2.2",
    "@ng-select/ng-select": "^2.9.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@ngxs/storage-plugin": "^3.2.0",
    "@ngxs/store": "^3.2.0",
    "@swimlane/ngx-datatable": "^13.1.0",
    "@uirouter/angular": "^1.1.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "date-fns": "^2.0.0-alpha.18",
    "ngx-resource-factory": "0.0.8",
    "ngx-ui-router-url-type-factory": "0.0.2",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.3",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "^6.1.9",
    "@angular/language-service": "^6.1.9",
    "@angularclass/hmr": "^2.1.3",
    "@babel/core": "^7.1.0",
    "@compodoc/compodoc": "^1.1.5",
    "@ngxs/devtools-plugin": "^3.2.0",
    "@storybook/addon-actions": "^4.0.0-alpha.23",
    "@storybook/addon-knobs": "^4.0.0-alpha.23",
    "@storybook/addon-notes": "^4.0.0-alpha.23",
    "@storybook/addon-options": "^4.0.0-alpha.23",
    "@storybook/angular": "^4.0.0-alpha.23",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.4",
    "@types/node": "~10.11.3",
    "babel-loader": "^8.0.4",
