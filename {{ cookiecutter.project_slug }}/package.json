{
  "name": "{{ cookiecutter.project_slug }}",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "app-start": "ng serve --host 0.0.0.0 --port 8080",
    "app-start-poll": "ng serve --host 0.0.0.0 --port 8080 --poll 1000",
    "app-start-hmr": "ng serve --host 0.0.0.0 --port 8080 -c=hmr",
    "app-start-hmr-poll": "ng serve --host 0.0.0.0 --port 8080 -c=hmr --poll 1000",
    "app-build-dev": "node helpers/version.js && ng build --base-href /",
    "app-build-stage": "node helpers/version.js && ng build -c=staging --base-href /",
    "app-build-prod": "node helpers/version.js && ng build -c=production --base-href /",
    "test": "node helpers/version.js && ng test --watch=false",
    "test-watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "makemessages": "ngx-translate-extract -i ./projects/{{ cookiecutter.project_slug }}/app/ -o ./projects/{{ cookiecutter.project_slug }}/assets/locales/*.json",
    "generatedocs": "./node_modules/.bin/compodoc -p ./projects/{{ cookiecutter.project_slug }}/tsconfig.app.json -d ./dist/docs",
    "storybook-start": "start-storybook --ci -p 9001 -h 0.0.0.0 -c ./projects/{{ cookiecutter.project_slug }}/storybook/config",
    "storybook-build": "node helpers/version.js && build-storybook -c ./projects/{{ cookiecutter.project_slug }}/storybook/config -o ./dist/storybook",
    "postinstall": "node helpers/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.14",
    "@angular/common": "^7.2.14",
    "@angular/compiler": "^7.2.14",
    "@angular/core": "^7.2.14",
    "@angular/forms": "^7.2.14",
    "@angular/http": "^7.2.14",
    "@angular/platform-browser": "^7.2.14",
    "@angular/platform-browser-dynamic": "^7.2.14",
    "@angular/router": "^7.2.14",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-regular-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@ng-bootstrap/ng-bootstrap": "^4.1.1",
    "@ng-select/ng-select": "^2.18.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/storage-plugin": "^3.4.3",
    "@ngxs/store": "^3.4.3",
    "@swimlane/ngx-datatable": "^14.0.0",
    "ngx-anx-forms": "^0.2.2",
    "@types/lodash": "^4.14.123",
    "@uirouter/angular": "^3.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^3.0.1",
    "date-fns": "^2.0.0-alpha.27",
    "lodash": "^4.17.11",
    "ngx-resource-factory": "0.0.11",
    "ngx-toastr": "^10.0.2",
    "ngx-ui-router-url-type-factory": "0.0.3",
    "rxjs": "6.5.1",
    "rxjs-compat": "6.5.1",
    "yarn": "^1.15.2",
    "zone.js": "~0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "^7.2.14",
    "@angular/language-service": "^7.2.14",
    "@angularclass/hmr": "^2.1.3",
    "@babel/core": "^7.4.3",
    "@compodoc/compodoc": "^1.1.9",
    "@ngxs/devtools-plugin": "^3.4.3",
    "@storybook/addon-actions": "~5.0.10",
    "@storybook/addon-knobs": "~5.0.10",
    "@storybook/addon-notes": "~5.0.10",
    "@storybook/addon-options": "~5.0.10",
    "@storybook/angular": "~5.0.10",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~11.13.7",
    "babel-loader": "^8.0.5",
    "core-js": "^3.0.1",
    "codelyzer": "~5.0.0",
    "css-loader": "^2.1.1",
    "git-describe": "^4.0.4",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "moment": "^2.24.0",
    "protractor": "~5.4.2",
    "puppeteer": "^1.14.0",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.2.4"
  }
}
