{
  "name": "{{ cookiecutter.project_slug }}",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "app-start": "ng serve --host 0.0.0.0 --port 8080",
    "app-start-poll": "ng serve --host 0.0.0.0 --port 8080 --poll 1000",
    "app-start-hmr": "ng serve --host 0.0.0.0 --port 8080 -c=hmr",
    "app-start-hmr-poll": "ng serve --host 0.0.0.0 --port 8080 -c=hmr --poll 1000",
    "app-build-dev": "node helpers/version.js && ng build --base-href /",
    "app-build-stage": "node helpers/version.js && ng build -c=staging --base-href /",
    "app-build-prod": "node helpers/version.js && ng build -c=production --base-href /",
    "test": "node helpers/version.js && ng test --watch=false",
    "test-watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "makemessages": "ngx-translate-extract -i ./src/app/ -o ./src/assets/locales/*.json",
    "generatedocs": "./node_modules/.bin/compodoc -p ./src/tsconfig.app.json -d ./dist/docs",
    "storybook-start": "start-storybook --ci -p 9001 -h 0.0.0.0 -c ./src/storybook/config",
    "storybook-build": "node helpers/version.js && build-storybook -c ./src/storybook/config -o ./dist/storybook",
    "postinstall": "node helpers/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.0",
    "@angular/common": "^7.1.0",
    "@angular/compiler": "^7.1.0",
    "@angular/core": "^7.1.0",
    "@angular/forms": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/platform-browser": "^7.1.0",
    "@angular/platform-browser-dynamic": "^7.1.0",
    "@angular/router": "^7.1.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.4.2",
    "@fortawesome/free-regular-svg-icons": "^5.4.2",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "@ng-select/ng-select": "^2.11.2",
    "@ngx-translate/core": "^11.0.0",
    "@ngx-translate/http-loader": "^4.0.0",
    "@ngxs/storage-plugin": "^3.2.0",
    "@ngxs/store": "^3.2.0",
    "@swimlane/ngx-datatable": "^14.0.0",
    "@uirouter/angular": "^2.0.2",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "date-fns": "^2.0.0-alpha.25",
    "ngx-resource-factory": "0.0.11",
    "ngx-toastr": "^9.1.1",
    "ngx-ui-router-url-type-factory": "0.0.3",
    "rxjs": "6.3.3",
    "rxjs-compat": "6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "^7.1.0",
    "@angular/language-service": "^7.1.0",
    "@angularclass/hmr": "^2.1.3",
    "@babel/core": "^7.1.0",
    "@compodoc/compodoc": "^1.1.5",
    "@ngxs/devtools-plugin": "^3.2.0",
    "@storybook/addon-actions": "~4.0.0",
    "@storybook/addon-knobs": "~4.0.0",
    "@storybook/addon-notes": "~4.0.0",
    "@storybook/addon-options": "~4.0.0",
    "@storybook/angular": "~4.0.0",
    "@types/jasmine": "~3.3.0",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~10.12.1",
    "babel-loader": "^8.0.4",
    "codelyzer": "~4.5.0",
    "css-loader": "^1.0.1",
    "git-describe": "^4.0.3",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "moment": "^2.22.2",
    "protractor": "~5.4.1",
    "puppeteer": "^1.9.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "tslint-sonarts": "^1.8.0",
    "typescript": "~3.1.3"
  }
}
