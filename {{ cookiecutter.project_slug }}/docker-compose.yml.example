version: '2'
services:
  node:
    build:
      context: ./docker/node
    volumes:
    - ./:/app
    command: npm run app-start
    ports:
    - "8080:8080"
    - "9876:9876"

  storybook:
    build:
      context: ./docker/node
    volumes:
    - ./:/app
    command: npm run storybook-start
    ports:
    - "9001:9001"
